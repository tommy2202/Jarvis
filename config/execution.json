{
  "enabled": true,
  "default_backend": "sandbox",
  "fallback_backend": "local_process",
  "sandbox": {
    "require_available": true
  },
  "allow_inline_intents": [
    "core.time.now",
    "core.date.today",
    "core.status.listening",
    "core.status.admin",
    "core.status.busy",
    "core.status.health",
    "core.identity.version"
  ]
}
